%h2 店舗登録
= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f|
  = render "devise/shared/error_messages", resource: resource
  .field
    = f.label :店名
    %br/
    = f.text_field :email, class:"form", autofocus: true, autocomplete: "旨い亭"
  .field
    = f.label :email
    %br/
    = f.email_field :email, class:"form", autofocus: true, autocomplete: "email"
  .field
    = f.label :password
    - if @minimum_password_length
      %em
        (#{@minimum_password_length} 文字以上)
    %br/
    = f.password_field :password, class:"form", autocomplete: "new-password"
  .field
    = f.label :password確認
    %br/
    = f.password_field :password_confirmation, class:"form", autocomplete: "new-password"
  .actions
    = f.submit "登録する" class: "submit_btn"
-# = render "restaurants/shared/links"
